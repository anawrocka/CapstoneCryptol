xs = [ 1 .. 100 ]

sumInf xs = iterSums
    where
        sums = [ 0 ] # [ x + y | x <- xs
                               | y <- sums ]
        iterSums = [ x + y | x <- sums
                           | y <- [ 1 ...] ]
